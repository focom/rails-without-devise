<div class="container">

  <%= form_with url: signin_url, local: true do |form| %>

    <p>
      <%= form.label :email %><br>
      <%= form.text_field :email %>
    </p>
    <p>
      <%= form.label :password %><br>
      <%= form.text_field :password %>
    </p>
    <p>
      <%= form.submit 'Login'%>
    </p>
  <% end %>
  <%= link_to "Reset password",  reset_password_url%>
</div>
